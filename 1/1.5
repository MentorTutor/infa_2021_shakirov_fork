import turtle
turtle.shape('turtle')
def square (x):
    for i in range (1,5):
        turtle.forward(x)
        turtle.left(90)
for i in range (10,220,20):
    square(i)
    turtle.right(90)
    turtle.penup()
    turtle.forward(10)
    turtle.right(90)
    turtle.forward(10)
    turtle.left(180)
    turtle.pendown()
